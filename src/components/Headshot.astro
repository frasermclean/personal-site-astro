---
import { Image } from 'astro:assets';
import imageFile from '../assets/headshot-dark-bg.png';

interface Props {
  size?: number;
}

const { size = 256 } = Astro.props;
---

<Image
  src={imageFile}
  alt="Headshot image of Fraser McLean"
  class="rounded-full"
  width={size}
  height={size}
/>
